  },
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-contrib-connect": "^1.0.2",
    "grunt-contrib-jshint": "^1.1.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-include-source": "^1.1.0",
    "grunt-wiredep": "^3.0.1",
    "jshint-stylish": "^2.2.1",
